" + supplier)\n  return (\n    <div>\n      <List\n        component=\"nav\"\n        aria-labelledby=\"nested-list-subheader\"\n        className={classes.root}\n      >\n        <ListItem button onClick={() => history.push(\"/dashboard\")}>\n          <ListItemIcon>\n            {/* company logo here */}\n            <BusinessIcon />\n          </ListItemIcon>\n          <ListItemText primary=\"Company Name\" />\n        </ListItem>\n        <hr class=\"MuiDivider-root\" style={{ marginBottom: \"10px\" }} />\n\n        <ListItem button onClick={handleClick}>\n          <ListItemIcon>\n            <PeopleIcon />\n          </ListItemIcon>\n          <ListItemText primary=\"My suppliers\" />\n          {open ? <ExpandLess /> : <ExpandMore />}\n        </ListItem>\n\n        {/* sub-menu for my suppliers here, create an iteration for user.suppliers and generate each Collapse element */}\n        {suppliers.map((element) => {\n          return (\n            <>\n              <Collapse in={open} timeout=\"auto\" unmountOnExit>\n                <List component=\"div\" disablePadding>\n                  <ListItem\n                    button\n                    className={classes.nested}\n                    onClick={() => \n                      getSupplierShow(history, element.id, element)\n                    }\n                  >\n                    {/* supplier logo here */}\n                    <ListItemIcon>\n                      {/* should be element.logo */}\n                      <PeopleIcon />\n                    </ListItemIcon>\n                    <ListItemText primary={element.name}/>\n                  </ListItem>\n                </List>\n              </Collapse>\n            </>\n          );\n        })}\n\n        <Collapse in={open} timeout=\"auto\" unmountOnExit>\n          <List component=\"div\" disablePadding>\n            <ListItem\n              button\n              className={classes.nested}\n              onClick={() => history.push(\"/suppliers/new\")}\n            >\n              <ListItemIcon>\n                <AddIcon color=\"primary\" />\n              </ListItemIcon>\n              <ListItemText primary=\"New supplier\" />\n            </ListItem>\n          </List>\n        </Collapse>\n\n        <ListItem button>\n          <ListItemIcon>\n            <ReceiptIcon />\n          </ListItemIcon>\n          <ListItemText primary=\"Payments\" />\n        </ListItem>\n        <ListItem button>\n          <ListItemIcon>\n            <BarChartIcon />\n          </ListItemIcon>\n          <ListItemText primary=\"Reports\" />\n        </ListItem>\n        <ListItem button>\n          <ListItemIcon>\n            <SearchIcon />\n          </ListItemIcon>\n          <ListItemText primary=\"Find suppliers\" />\n        </ListItem>\n      </List>\n    </div>\n  );\n}\n\nexport const secondaryListItems = (\n  <div>\n    <ListSubheader inset>Secondary List Items</ListSubheader>\n    <ListItem button>\n      <ListItemIcon>\n        <PeopleIcon />\n      </ListItemIcon>\n      <ListItemText primary=\"About Us\" />\n    </ListItem>\n    <ListItem button>\n      <ListItemIcon>\n        <PeopleIcon />\n      </ListItemIcon>\n      <ListItemText primary=\"Contact\" />\n    </ListItem>\n    <ListItem button>\n      <ListItemIcon>\n        <PeopleIcon />\n      </ListItemIcon>\n      <ListItemText primary=\"TBD\" />\n    </ListItem>\n  </div>\n);\n","/Users/mark/Desktop/CA_PROJECTS/Study folder/T3A2-B_full_stack_app/supi-supplier-management-app-frontend/src/components/invoice/NewInvoice.jsx",["143","144"],"import React, { useReducer } from \"react\";\nimport reducer from \"../../utils/reducer\";\nimport Grid from \"@material-ui/core/Grid\";\nimport { Form } from \"../styles/Form\";\nimport FormContainer from \"../styles/FormContainer\";\nimport Button from \"@material-ui/core/Button\";\n\nfunction NewInvoice({ history, match }) {\n  const initialInvoiceState = {\n    receivedDate: \"\",\n    dueDate: \"\",\n    